<Document id="NeoGeoWebTouring">
<name>Ebola Outbreaks along time</name>
<open>1</open>
<description>Touring over Ebola outbreaks since 1976. Ballons show details on each epidemics.</description>

<Style id="tassiastyle">
	<open>1</open>
	<LineStyle>
		<color>ff0000ff</color>
	</LineStyle>
	<BalloonStyle>
		<bgColor>#dddddd</bgColor>
		<text>
			<![CDATA[
				<table width="250" border="0" cellpadding="5" cellspacing="0" bgcolor="#ffffff">
				<tr>
					<td>
						<img src=$[flag_src] alt="picture" width="60" align="left" />
					</td>
					<td>
						<h3><font color="#006600">$[country]</font></h3>
					</td>
				</tr>
				<tr>
					<td></td><td align="center" ><b>$[year]</b></td>
				</tr>				
				<tr>
					<td colspan="2">
						<p>$[info]</p>
					</td>
				</tr>
				<table border="0.5" cellpadding="5">
					<tr><td>Cases</td><td>Deaths</td><td>Fatality</td></tr>
					<tr align="center"><td><b>$[cases]</b></td><td><b>$[deaths]</b></td><td><b>$[fatality]</b></td></tr>
				</table>
			]]>
		</text>
	</BalloonStyle>
</Style>

<Style id="annastyle">
    <BalloonStyle>
        <bgColor>503C0014</bgColor>
        <text>
            <![CDATA[
                <br/>
				<table width="100" border="0" cellpadding="10" cellspacing="0">
					<tr>
						<th align="left" ><font size="4">$[name]</font></th>
						<th>dsfdsfd</th>
						<th rowspan="2"><img src=$[flag] alt="picture" width="50"</th>
					</tr>
					<tr>
						<td><b><font size="3">$[year]</font></b></td>
						<td></td>
					</tr>
					<tr align="justify">
						<td colspan="3"><font size="2">Strain: $[strain]<br/></br>$[description]</font></td>
					</tr>
				</table>
				<br/>
				<table width="290" border="0" cellpadding="3" cellspacing="0" bgcolor="#bebebe">
					<tr align="center">
						<td><font size="3">Cases</font></td>
						<td><font size="3">Deaths</font></td>
						<td><font size="3">Fatality</font></td>
					</tr>
					<tr align="center">
						<td><font size="2"><b>$[cases]</b></font></td>
						<td><font size="2"><b>$[deaths]</b></font></td>
						<td><font size="2"><b>$[fatality]</b></font></td>
					</tr>
				</table>
				<br/>
            ]]>
        </text> 
    </BalloonStyle>
    <Polystyle>
        <fill>1</fill>
    </Polystyle>
    <LineStyle>
        <color>ff0000ff</color>
    </LineStyle>
</Style>

<Style id="rachelstyle">
    <BalloonStyle>
      <!-- a background color for the balloon -->
      <bgColor>ffffffbb</bgColor>
      <!-- styling of the balloon text -->
      <text>
        <![CDATA[
        <b><font color="#CC0000" size="+2">$[name]</font></b>
        <br/><br/>
        <td>
        <img src=$[flag] alt="picture" width="60" align="left" />
        </td>
        <font face="Courier">$[description]</font>
        <br/><br/>
        <br/><br/>
        <table border="0.5" cellpadding="5">
					<tr><td>Cases</td><td>Deaths</td><td>Fatality</td></tr>
                    <tr align="center"><td><b>$[cases]</b></td><td><b>$[deaths]</b></td><td><b>$[fatality]</b></td></tr>
				</table>
        <!-- insert the to/from hyperlinks -->
        ]]>
      </text>
    </BalloonStyle>
    <LineStyle>
       <color>ff0000ff</color>
    </LineStyle>
    <Polystyle>
            <fill>1</fill>
        </Polystyle>
  </Style>
  
<Style id="raw">
    <BalloonStyle>
      <text>
        <![CDATA[$[description]]]>
      </text>
    </BalloonStyle>
</Style>
  
